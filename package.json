{
  "name": "todo-list-in-palette",
  "displayName": "TODO List in Palette",
  "description": "TODO list in the Command Palette",
  "version": "0.0.2",
  "publisher": "FoxLabs",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "todo-list.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "todo-list.combobulate",
        "title": "DiscombobulateRR"
      },
      {
        "command": "todo-list.showList",
        "title": "Show List",
        "category": "TODO"
      },
      {
        "command": "todo-list.addTodo",
        "title": "Add Task",
        "category": "TODO"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
